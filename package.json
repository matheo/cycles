{
  "name": "@matheo/cycles",
  "version": "0.0.1",
  "homepage": "https://github.com/matheo/cycles/",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "mobile:plugin": "yarn --cwd apps/mobile plugin",
    "mobile:cap": "yarn --cwd apps/mobile cap",
    "mobile:run": "ionic capacitor run -l --consolelogs --address 0.0.0.0 --project mobile",
    "mobile:serve": "ionic serve --consolelogs --address 0.0.0.0 --project mobile",
    "mobile:sync": "cd apps/mobile && rimraf node_modules && yarn && yarn cap:sync",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "lint": "nx workspace-lint && ng lint",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "e2e": "ng e2e",
    "test": "ng test",
    "ng": "nx",
    "nx": "nx",
    "postinstall": "ngcc && node ./decorate-angular-cli.js"
  },
  "config": {
    "save-prefix": "~"
  },
